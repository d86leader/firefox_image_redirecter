{ "manifest_version": 2
, "name": "image_redirecter"
, "version": "1.0"

, "description": "Redirect image pages to images"

, "permissions":
    [ "webRequest"
    , "webRequestBlocking"

    , "https://imgur.com/*"
    , "https://i.imgur.com/*"
    ]

, "background":
    { "scripts": ["./build/background.js"]
    }
}
